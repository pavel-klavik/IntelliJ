;; Deletes one character forward of the caret. The position of the caret will stay the same.
(defmethod op/editor-op :editor/delete-forward [_ effects _ _]
  (if (selection-util/caret? effects)
    (let [{:editor-selection/keys [anchor-id anchor-offset]} (db-get/editor-selection effects)
          last-offset (db-segment/last-offset? effects anchor-id anchor-offset)
          last-segment (db-segment/last? effects anchor-id)]
      (if (and last-segment last-offset)
        (delete-newline-forward effects)
        (delete-character-forward effects)))
    {:updated-effects effects}))</text>
