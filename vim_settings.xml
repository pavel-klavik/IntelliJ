     :new-ops         (when next-paragraph-id
                        [[:paragraph/join #:paragraph{:ids [paragraph-id next-paragraph-id]}]])}))

(defn- delete-character-forward
  &quot;Adds operation to delete a single character forward. If the caret occurs at the end of the current segment,
  the character is deleted from the following segment, moving the caret there.&quot;
  [effects]
  (let [{:editor-selection/keys [anchor-id anchor-offset]} (db-get/editor-selection effects)
        last-offset (db-segment/last-offset? effects anchor-id anchor-offset)]
    (if (not last-offset)
      {:updated-effects (db-update/editor-selection effects assoc :editor-selection/force-update true
                                                    :editor-selection/auto-direction :editor-selection/forward)
       :new-ops         [[:segment/remove-text #:segment{:id     anchor-id
                                                         :offset anchor-offset
                                                         :length 1}]]}
      (let [next-segment-id (db-segment/next-segment effects anchor-id)]
        {:updated-effects (db-assoc/editor-selection
                            effects #:editor-selection{:anchor-id      next-segment-id
                                                       :anchor-offset  0
                                                       :force-update   true
                                                       :auto-direction :editor-selection/forward})
